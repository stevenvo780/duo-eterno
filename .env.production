VITE_APP_VERSION="1.0.0"
VITE_ENVIRONMENT="production"

# URLs de producción
VITE_BASE_URL="https://duo-eterno.vercel.app"

# Configuración de analytics (cuando se implemente)
# VITE_ANALYTICS_ID=""

# Configuración de features
VITE_ENABLE_DEV_TOOLS="false"
