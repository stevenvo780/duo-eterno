# Configuración para debugging rápido
# Copia estos valores a tu .env para acelerar el desarrollo y debugging

# Modo turbo para testing rápido - eventos muy acelerados
VITE_GAME_SPEED_MULTIPLIER=5.0
VITE_GAME_CLOCK_INTERVAL=200
VITE_STAT_DECAY_SPEED=5.0
VITE_AUTOPOIESIS_INTERVAL=1000
VITE_ACTIVITY_CHANGE_FREQUENCY=0.5
VITE_ZONE_EFFECTS_INTERVAL=500
VITE_DIALOGUE_DURATION=1000
VITE_CRITICAL_EVENT_PROBABILITY=0.1
VITE_DEBUG_MODE=true

# Usar versión optimizada automáticamente
VITE_USE_OPTIMIZED=true

# FPS más bajo para mejor rendimiento durante debug
VITE_TARGET_FPS=30
VITE_MOVEMENT_UPDATE_FPS=20
