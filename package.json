{
  "name": "duo-eterno",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "find-unused": "ts-prune --project tsconfig.app.json",
    "server": "node backend/server.js",
    "dev-full": "concurrently \"npm run server\" \"npm run dev\"",
    "show-stats": "node scripts/show-stats.js",
    "analyze-session": "node scripts/analyzeSession.js",
    "analyze-now": "node scripts/analyze-now.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^4.21.2",
    "globals": "^16.0.0",
    "happy-dom": "^16.5.3",
    "jsdom": "^24.1.1",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^2.1.8"
  }
}
